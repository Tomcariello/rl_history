<section class="main">
  {{!-- <div class='row pageTitle'>
    <div class="col-sm-12">
        Current Research
    </div>
  </div> --}}

  {{#each dynamicData}}
    <div class='row'>
      <div class="col-xs-12 pageTitle">
        {{{this.header}}}
      </div>
    </div>
    <div class="row">
      <div class="col-md-2"></div>
      <div class="col-md-8">
        <img class="profile" src="{{this.elementimage}}">
        <Br>
        {{this.imagecaption}}
      </div>
      <div class="col-md-2"></div>
    </div>
      


    <div class='row'>
      <div class="col-md-1"></div>
      <div class="col-sm-12 col-md-10">
        <p>
          {{{this.elementtext}}}
        </p>
      </div>
      <div class="col-md-1"></div>
    </div>
  {{/each}}
</section>