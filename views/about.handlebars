<section class="main">
  {{#each dynamicData}}
    <div class='row'>
      <div class="col-xs-12 pageTitle">
        {{{this.header}}}
      </div>
    </div>

    <div class='row'>
      <div class="col-sm-12 col-md-7">
        <p>
          {{{this.elementtext}}}
        </p>
      </div>

      <div class="col-md-5">
        <div>
            <img class="profile" src="{{this.elementimage}}" alt="">
        </div>
      </div>
    </div>
  {{/each}}

</section>